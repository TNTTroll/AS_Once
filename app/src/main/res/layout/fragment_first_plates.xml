<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Puzzles.FirstPlates">

    <com.example.onceuponatime.Object
        android:id="@+id/firstPlatesWallBG"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:background="@drawable/bg_wall"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.example.onceuponatime.Object
        android:id="@+id/firstPlatesBack"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/back"
        android:contentDescription="@string/app_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.example.onceuponatime.Object
        android:id="@+id/firstPlate_1"
        android:layout_width="885px"
        android:layout_height="630px"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="100dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/plates_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <com.example.onceuponatime.Object
        android:id="@+id/firstPlate_2"
        android:layout_width="228px"
        android:layout_height="270px"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/plates_2"
        app:layout_constraintEnd_toStartOf="@+id/firstPlate_3"
        app:layout_constraintTop_toBottomOf="@+id/firstPlate_1"
        tools:ignore="MissingConstraints" />

    <com.example.onceuponatime.Object
        android:id="@+id/firstPlate_3"
        android:layout_width="649px"
        android:layout_height="265px"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/plates_3"
        app:layout_constraintEnd_toStartOf="@+id/firstPlate_5"
        app:layout_constraintTop_toBottomOf="@+id/firstPlate_1"
        tools:ignore="MissingConstraints" />

    <com.example.onceuponatime.Object
        android:id="@+id/firstPlate_4"
        android:layout_width="383px"
        android:layout_height="280px"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/plates_4"
        app:layout_constraintStart_toEndOf="@+id/firstPlate_1"
        app:layout_constraintTop_toTopOf="@+id/firstPlate_1"
        tools:ignore="MissingConstraints" />

    <com.example.onceuponatime.Object
        android:id="@+id/firstPlate_5"
        android:layout_width="378px"
        android:layout_height="611px"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/plates_5"
        app:layout_constraintStart_toEndOf="@+id/firstPlate_1"
        app:layout_constraintTop_toBottomOf="@+id/firstPlate_4"
        tools:ignore="MissingConstraints" />

    <com.example.onceuponatime.Object
        android:id="@+id/firstHourArrow"
        android:layout_width="505px"
        android:layout_height="505px"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/arrow_hour"
        android:contentDescription="@string/app_name"
        app:layout_constraintEnd_toEndOf="@+id/firstPlate_1"
        app:layout_constraintHorizontal_bias="0.575"
        app:layout_constraintStart_toStartOf="@+id/firstPlate_1"
        app:layout_constraintTop_toTopOf="@+id/firstPlate_1"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>